gin: 0; padding: 0; } html { scroll-behavior: smooth; -webkit-text-size-adjust: 100%; } body { line-height: 1.6; font-family: var(--font-family); } /* ==================================== HEADER Y NAVEGACIÓN ==================================== */ /* Estructura principal del header */ .site-header { position: fixed; top: 0; left: 0; right: 0; width: 100%; margin-inline: auto; z-index: 1000; transition: all var(--transition); } .site-header.scrolled { box-shadow: var(--shadow); top: 0; width: 100%; } .site-header:not(.scrolled) .nav-link { color: var(--accion); } .site-header.scrolled .nav-link { color: var(--accion); } .site-header:not(.scrolled) .navegacion { background-color: transparent; } .site-header.scrolled .navegacion { background-color: var(--bg-white); } /* Banner superior con información de contacto */ .banner_nav { display: flex; justify-content: flex-end; align-items: center; gap: 8px 20px; padding: 0.85rem 12px; background-color: var(--bg-white); border-bottom: 1px solid var(--neutral-gray-dark); } .banner_nav a { display: flex; align-items: center; gap: 8px; text-decoration: none; transition: color var(--transition); font-weight: 400; } .banner_nav a span { font-size: 18px; padding: 0; } .banner_nav a span:hover { color: var(--primary); transform: translateY(-2px); text-decoration: none; } .banner_nav svg { width: 20px; height: 20px; } /* Barra de navegación principal */ .navegacion { position: relative; display: flex; align-items: center; justify-content: space-between; padding: 4px 12px; margin-left: auto; gap: 1em; } /* Logo */ .logo_nav { display: flex; align-items: center; gap: 8px; text-decoration: none; z-index: 10; transition: transform var(--transition); } .logo_nav img { height: 60px; width: auto; background-color: white; border-radius: 50%; padding: 2px; } .logo_nav img span { display: none; font-size: 18px; font-weight: 700; } .logo_nav:hover { transform: scale(1.05); } /* Enlaces de navegación */ .nav-links { display: flex; align-items: center; gap: 2em; margin-left: auto; font-size: 18px; font-weight: 700; } .nav-link { color: var(--text-dark); text-decoration: none; transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275); } /* Dropdown base */ .dropdown { position: relative; transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275); cursor: pointer; } .dropdown-content { display: none; position: absolute; top: 28px; left: 0; min-width: 240px; background-color: var(--text-light); box-shadow: 0px 8px 16px 0px rgba(0, 0, 0, 0.2); padding: 20px; z-index: 100; border-radius: var(--border-radius); } /* Megamenú de servicios */ #services-megamenu { position: fixed; top: calc(var(--banner-height) + var(--header-height) - 15px); left: 0; right: 0; width: 100%; background-color: var(--bg-white); box-shadow: var(--shadow); z-index: 99; display: none; padding: 30px 0; opacity: 0; transform: translateY(-10px); transition: opacity 0.3s ease, transform 0.3s ease; } .megamenu-content { max-width: 1280px; margin: 0 auto; padding: 0 20px; } .megamenu-columns { display: grid; grid-template-columns: repeat(auto-fit, minmax(250px, 1fr)); gap: 30px; } /* Categorías de servicios */ .service-category { margin-bottom: 15px; } .service-category h4 { color: var(--accion); margin-bottom: 10px; font-size: 20px; border-bottom: 1px solid rgba(0, 0, 0, 0.1); transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275); } .service-category h4:hover { color: var(--primary); } .service-links { display: grid; gap: 8px; } /* Enlaces dentro de dropdowns */ .service-category a, .dropdown-content a { color: var(--text-dark); text-decoration: none; transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275); border-bottom: 1px solid rgba(0, 0, 0, 0.05); font-weight: 400; } .service-category a:hover, .dropdown-content a:hover { transform: translateY(-2px); color: var(--primary); } /* Selector de idioma */ .language-switcher { margin-left: 0.75rem; } .language-switcher a { display: flex; align-items: center; gap: 0.25rem; font-weight: 400; font-size: 1rem; padding: 0.5rem 0.75rem; border-radius: 8px; background-color: var(--crystal-clear); color: var(--color-gray); text-decoration: none; transition: background-color var(--transition); } .language-switcher a:hover { background-color: rgba(0, 0, 0, 0.1); } /* Botón móvil */ .mobile-nav-toggle { display: none; background: transparent; border: none; cursor: pointer; z-index: 1001; padding: 0; } .mobile-nav-toggle svg line { transition: all 0.3s ease; } .mobile-nav-toggle.active svg line:nth-child(1) { transform: translateY(9px) rotate(45deg); transform-origin: center; } .mobile-nav-toggle.active svg line:nth-child(2) { opacity: 0; } .mobile-nav-toggle.active svg line:nth-child(3) { transform: translateY(-9px) rotate(-45deg); transform-origin: center; } /* Animaciones */ @-webkit-keyframes fadeIn { from { opacity: 0; transform: translateY(-10px); } to { opacity: 1; transform: translateY(0); } } @keyframes fadeIn { from { opacity: 0; transform: translateY(-10px); } to { opacity: 1; transform: translateY(0); } } .dropdown, .nav-link { -webkit-animation: fadeIn 0.3s ease-out forwards; animation: fadeIn 0.3s ease-out forwards; } /* ==================================== MEDIA QUERIES - MOBILE FIRST ==================================== */ /* Versión móvil (por defecto) */ @media screen and (max-width: 1120px) { /* Mostrar botón móvil */ .mobile-nav-toggle { display: block; } /* Ocultar nombre en logo */ .logo_nav span { display: none; } /* Menú móvil base */ .nav-links { position: fixed; top: calc(var(--banner-height) + var(--header-height)); left: 0; right: 0; background-color: var(--dark); flex-direction: column; align-items: flex-start; height: 0; overflow: hidden; transition: height 0.4s cubic-bezier(0.4, 0, 0.2, 1); padding: 0; box-shadow: var(--shadow); z-index: 100; } /* Menú móvil abierto */ .nav-links-mobile { height: calc(100vh - var(--banner-height) - var(--header-height)); overflow-y: visible; padding: 20px; } /* Elementos de navegación en móvil */ .dropdown, .nav-link { width: 100%; border-bottom: 1px solid var(--border-color); padding: 15px 0; } /* Estilo para dropdown en móvil */ .dropdown > span { display: flex; justify-content: space-between; align-items: center; } /* Añadir indicador de flecha en móvil */ .dropdown > span::after { content: '▼'; font-size: 12px; transition: transform 0.3s; margin-left: 8px; } /* Rotar flecha cuando está activo */ .dropdown.active > span::after { transform: rotate(180deg); } /* Megamenú en móvil */ #services-megamenu { position: static; display: none; padding: 0; box-shadow: none; transform: none; opacity: 1; } .megamenu-content { padding: 0; } .megamenu-columns { grid-template-columns: 1fr; gap: 0; } .site-header:not(.scrolled) .nav-link { color: var(--text-dark); /* En móvil siempre oscuro */ } /* Mostrar megamenú cuando el dropdown de servicios está activo */ #services-dropdown.active ~ #services-megamenu { display: block; padding: 10px 0 10px 20px; } /* Dropdown normal activo en móvil */ .dropdown.active .dropdown-content { display: grid; padding: 10px 0 10px 20px; } .service-category { margin-bottom: 10px; } .service-category h4 { font-size: 1rem; margin-bottom: 5px; } /* Banner móvil */ .banner_nav { justify-content: center; flex-wrap: wrap; } .banner_nav a:last-child { display: none; } } /* ==================================== VERSIÓN ESCRITORIO ==================================== */ /* Pantallas medianas y grandes */ @media screen and (min-width: 1024px) { /* Mostrar nombre en logo */ .logo_nav span { display: inline-block; } } /* Específico para escritorio */ @media screen and (min-width: 1121px) { /* Efectos hover para dropdowns */ .dropdown:hover { transform: scale(1.05); } /* Mostrar contenido de dropdown al hover */ .dropdown:hover .dropdown-content { display: grid; gap: 8px; } } /************* OTROS ESTILOS GENERALES **************/ h1, h2, h3, h4, h5{ line-height: var(--line-height); text-transform: none; max-width: 600px; } h1 { font-size: var(--font-size-h1); } h2{ color: var(--primary); font-size: var(--font-size-h2); } h3 { font-size: var(--font-size-h3); color: var(--color-gray); } h4 { font-size: var(--font-size-h4); } h5{ font-size: var(--font-size-h5); } p{ font-size: var(--font-size-p); width: min(100%, 600px); margin-bottom: 1rem; } ul, ol{ margin-left: 2em; } li { width: min(100%, 600px); } ol li { margin-left: 3em; list-style: decimal; } ::marker { font-size: 2rem; color: var(--secundary); } a{ text-decoration: none; color: var(--accion); transition: all 0.3s cubic-bezier(0.175, 0.885, 0.32, 1.275); font-weight: 700; &:hover{ text-decoration: underline; text-decoration-thickness: 2.4px; text-underline-offset: 0.4em; color: var(--primary); } } img { border-style: none; max-width: 100%; height: auto; box-shadow: var(--shadow); border-radius: var(--border-radius-imgs); } /* Responsive Wrapper */ .wrapper { padding: 8em 0; width: min(calc(100% - 2rem), 1280px); margin-inline: auto; } /* Flow Utility for Spacing */ .flow > * + * { margin-top: 1.5em; } /* Two Columns Layout */ .two_columns { display: flex; flex-wrap: wrap; gap: 3em; } .two_columns > div, .two_columns figure { flex: 1 0 400px; max-width: 100%; } /* .two_columns img { height: auto; } */ .two_columns-image { line-height: 0; } .two_columns_left { flex-direction: row-reverse; } /* Three Columns Layout */ .three_columns { display: flex; flex-wrap: wrap; gap: 1em; justify-content: space-between; align-items: flex-start; padding: 8em 0 4em; & > div { position: relative; width: min(100%, 376px); display: grid; place-items: center; overflow: hidden; } } /* WhatsApp Button */ .boton-whatsapp { position: fixed; bottom: 8rem; right: 12px; z-index: 100; -webkit-animation: scale-up-center 0.4s cubic-bezier(0.390, 0.575, 0.565, 1.000) 0.5s 6 alternate-reverse both; animation: scale-up-center 0.4s cubic-bezier(0.390, 0.575, 0.565, 1.000) 0.5s 6 alternate-reverse both; transition: all .2s ease-in-out; width: -webkit-max-content; width: -moz-max-content; w